  <h1>Danh sách tài khoản</h1>
  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Id</th>
        <th>Họ và tên</th>
        <th>Ảnh</th>
        <th>Email</th>
        <th>Mật khẩu</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
        <tr>
          <td>{{this._id}}</td>
          <td>{{this.name}}</td>
          <td><img src={{this.img}} alt="" width="80" height="80"></td>
          <td>{{this.email}}</td>
          <td>{{this.password}}</td>
          <td><button type="button" class="btn btn-danger">Xóa</button></td>
          <td><button type="button" class="btn btn-success">Sửa</button></td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>